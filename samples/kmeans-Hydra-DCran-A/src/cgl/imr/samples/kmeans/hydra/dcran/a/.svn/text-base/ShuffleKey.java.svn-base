package cgl.imr.samples.kmeans.hydra.dcran.a;

import cgl.imr.types.IntKey;

/*
 * Intermediate Integer Key
 */
public class ShuffleKey extends IntKey {
	public ShuffleKey() {
		super();
	}

	public ShuffleKey(int key) {
		super(key);
	}

	public boolean isMergeableInShuffle() {
		return true;
	}
}
